{
  "name": "writing-website",
  "version": "1.0.0",
  "private": true,
  "description": "writing-website",
  "author": "Paul Wilson",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "prepare": "husky install",
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "audit": "audit-ci --config ./.audit-ci.json",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx --rulesdir ./node_modules/gatsby/dist/utils/eslint-rules",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint src/**/*.scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "check-updates": "ncu --configFileName .check-updates.json",
    "check-updates:fix": "ncu --configFileName .check-updates.json --upgrade --errorLevel 1 && npm install",
    "check-updates:all": "ncu --configFileName .check-updates.json --target latest --errorLevel 1"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.3",
    "bootstrap": "^5.2.1",
    "gatsby": "^4.24.0",
    "gatsby-plugin-google-analytics": "^4.24.0",
    "gatsby-plugin-image": "^2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-mdx": "^4.3.0",
    "gatsby-plugin-netlify-cms": "^6.24.0",
    "gatsby-plugin-sass": "^5.24.0",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-transformer-sharp": "^4.24.0",
    "netlify-cms-app": "^2.15.72",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "sass": "^1.55.0",
    "stylelint-config-recommended-scss": "^7.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@types/node": "^18.7.23",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "audit-ci": "^6.3.0",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-import-quotes": "^0.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "gatsby-plugin-eslint": "^4.0.2",
    "husky": "^8.0.1",
    "netlify-cli": "^12.0.0",
    "npm-check-updates": "^16.3.2",
    "postcss-scss": "^4.0.5",
    "stylelint": "^14.12.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.3"
  }
}
